{
  "manifest_version": 2,
  "name": "Mr.Screeny",
  "description": "This extension injects js",
  "version": "1.0",
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "<all_urls>",
    "storage",
    "activeTab"
  ]
}